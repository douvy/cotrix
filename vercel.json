{
  "version": 2,
  "builds": [
    {
      "src": "api/src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/coupons",
      "dest": "api/src/server.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/src/server.ts"
    }
  ],
  "functions": {
    "api/**/*": {
      "memory": 3008,
      "maxDuration": 60
    }
  }
}