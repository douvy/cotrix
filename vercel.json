{
    "version": 2,
    "buildCommand": "npm run build",
    "builds": [
      {
        "src": "dist/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/coupons",
        "dest": "dist/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "dist/server.js"
      }
    ]
  }